#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = N2Server N2Client
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

N2Server: Server\N2Server.bpr
  $(MAKE)

N2Client: Client\N2Client.bpr
  $(MAKE)


