(defun last (l) (car (reverse l)))
(last '(a b c d))
(defun shl (l) (append(cdr l)(list(car l))))
(shl '(a b c d))
(defun shr (l) (append(list(last l))(reverse(cdr(reverse l)))))
(shr '(a b c d))
(defun palindrom(l)(append l (cdr(reverse l))))
(palindrom '(a b c))
